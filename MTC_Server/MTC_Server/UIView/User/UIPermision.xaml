             xmlns:local="clr-namespace:MTC_Server.UIView.User"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="300" FontFamily="/MTC_Server;component/Asset/Fonts/#Myriad Pro" FontSize="10.667">
